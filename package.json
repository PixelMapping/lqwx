{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.10",
    "add": "^2.0.6",
    "antd-mobile": "^2.3.3",
    "js-cookie": "^2.2.1",
    "lib-flexible": "^0.3.2",
    "lint-staged": "^10.0.7",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px2rem-exclude": "0.0.6",
    "prettier": "^1.19.1",
    "rc-form": "^2.4.11",
    "react": "^16.13.1",
    "react-canvas-draw": "^1.1.1",
    "react-dom": "^16.13.1",
    "react-wx-images-viewer": "^1.0.6",
    "umi": "^3.2.10",
    "vue-esign": "^1.0.5",
    "yorkie": "^2.0.0"
  }
}
